<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
      <supportedRuntime version="v2.0.50727"/>
    </startup>
  <appSettings>
    
    <!--Special keys
      Do not change the name of these keys, only the value
      File == file to run
      Args == Command line arguments
      REQ_Quit == Path to send request to quit mailpile
    -->
    <add key="File" value="cmd.exe"/>
    <add key="Args" value=""/>
    <add key="REQ_Quit" value=""/>
    <!-- 
      key   == name that shows up in a list of available options,
               REQ_ prefix means that a request will be sent but a browser will not be opnened  
      value == path that opens in system browser when option key is clicked
    -->
    <add key="Google" value="http://www.google.com"/>
    <add key="HÍ" value="http://hi.is"/>
    <add key="REQ_xkcd" value="https://xkcd.com"/>
  </appSettings>
</configuration>